<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Test Kakao Map API Key</title>
    <script src="https://dapi.kakao.com/v2/maps/sdk.js?appkey=3585dfdc26eb8686b36cb652a1410094"></script>
    <style>
        #map {
            width: 100%;
            height: 500px;
        }
    </style>
</head>
<body>
    <h1>Kakao Map Test</h1>
    <div id="map"></div>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            if (typeof kakao !== 'undefined' && kakao.maps) {
                var mapContainer = document.getElementById('map');
                var mapOptions = {
                    center: new kakao.maps.LatLng(37.5665, 126.978),
                    level: 3
                };
                var map = new kakao.maps.Map(mapContainer, mapOptions);
            } else {
                console.error('Kakao Maps API is not loaded or kakao object is undefined.');
            }
        });
    </script>
</body>
</html>
